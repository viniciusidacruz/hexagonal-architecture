datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator db {
    provider = "go run github.com/steebchen/prisma-client-go"
    output   = "../infra/prisma/db"
}

model Product {
    id     String @id @default(uuid())
    name   String
    price  Float
    status String
}
